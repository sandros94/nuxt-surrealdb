{
  "name": "nuxt-surrealdb",
  "version": "0.4.1",
  "description": "A Nuxt module aimed to simplify the use of SurrealDB",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "surrealdb"
  ],
  "repository": "https://github.com/sandros94/nuxt-surrealdb",
  "homepage": "https://nuxt-surrealdb.s94.dev",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Sandro Circi",
    "email": "sandro.circi@digitoolmedia.com"
  },
  "build": {
    "externals": [
      "defu",
      "ofetch",
      "ufo"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "nr lint && nr prepack && changelogen --release --push --publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.2",
    "@vueuse/core": "^11.1.0",
    "undio": "^0.2.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.5.2",
    "@nuxt/eslint-config": "^0.7.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "@nuxt/test-utils": "^3.14.3",
    "changelogen": "^0.5.7",
    "eslint": "^9.12.0",
    "nuxt": "^3.13.2",
    "vite": "^5.4.8",
    "vitepress": "^1.3.4",
    "vitest": "^2.1.2",
    "vue-tsc": "^2.1.6"
  },
  "resolutions": {
    "nuxt-surrealdb": "workspace:*"
  }
}
