{
  "name": "nuxt-surrealdb",
  "version": "0.5.0-alpha.7",
  "description": "A Nuxt module aimed to simplify the use of SurrealDB",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "surrealdb"
  ],
  "repository": "https://github.com/sandros94/nuxt-surrealdb",
  "homepage": "https://nuxt-surrealdb.s94.dev",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Sandro Circi",
    "email": "sandro.circi@digitoolmedia.com"
  },
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ]
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground && nuxt prepare docs",
    "docs": "nuxt dev docs",
    "docs:build": "nuxt build docs",
    "docs:preview": "nuxt preview docs",
    "release": "pnpm lint && pnpm test && pnpm prepack && changelogen --release --push --publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.0",
    "defu": "^6.1.4",
    "exsolve": "^1.0.7",
    "surrealdb": "2.0.0-alpha.10"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@surrealdb/node": "^2.3.3",
    "@surrealdb/wasm": "^2.3.3",
    "changelogen": "^0.6.2",
    "eslint": "^9.37.0",
    "nuxt": "^4.2.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  },
  "resolutions": {
    "nuxt-surrealdb": "workspace:*"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "packageManager": "pnpm@10.18.2"
}
